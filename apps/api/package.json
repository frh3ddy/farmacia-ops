{
  "name": "@farmacia-ops/api",
  "version": "1.0.0",
  "scripts": {
    "build": "node -e \"const {execSync}=require('child_process');const path=require('path');const tsc=require.resolve('typescript/bin/tsc');const pkgDir=path.dirname(require.resolve('./package.json'));process.chdir(pkgDir);execSync('node '+tsc,{stdio:'inherit'})\"",
    "start": "node dist/index.js"
  },
  "dependencies": {
    "@farmacia-ops/shared": "file:../shared"
  },
  "devDependencies": {
    "@types/node": "^25.0.3",
    "typescript": "^5.0.0"
  }
}