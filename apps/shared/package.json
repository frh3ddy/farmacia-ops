{
  "name": "@farmacia-ops/shared",
  "version": "1.0.0",
  "description": "Shared library for Farmacia Ops",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "node -e \"const {execSync}=require('child_process');const path=require('path');const fs=require('fs');const tsc=require.resolve('typescript/bin/tsc');const pkgDir=path.dirname(require.resolve('./package.json'));execSync('node '+tsc+' -p '+path.join(pkgDir,'tsconfig.json'),{stdio:'inherit'})\"",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "bullmq": "^5.0.0",
    "ioredis": "^5.3.2"
  },
  "devDependencies": {
    "@types/node": "^25.0.3",
    "typescript": "^5.0.0"
  }
}

