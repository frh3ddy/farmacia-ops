    "start": "node dist/worker.js",
    "test:connection": "ts-node src/test-connection.ts"
  },
  "dependencies": {
    "@farmacia-ops/shared": "file:../shared",
    "bullmq": "^5.0.0",
    "ioredis": "^5.3.2"
  },
  "devDependencies": {
    "@types/node": "^25.0.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.0.0"
  }
}