{
  "name": "Farmacia-ops",
  "version": "1.0.0",
  "description": "",
  "homepage": "https://github.com/frh3ddy/farmacia-ops#readme",
  "bugs": {
    "url": "https://github.com/frh3ddy/farmacia-ops/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/frh3ddy/farmacia-ops.git"
  },
  "license": "ISC",
  "author": "",
  "type": "commonjs",
  "main": ".eslintrc.js",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "npm run build --workspace=packages/shared && npm run build --workspace=apps/worker && npm run build --workspace=apps/api",
    "build:shared": "npm run build --workspace=packages/shared",
    "build:worker": "npm run build:shared && npm run build --workspace=apps/worker",
    "build:api": "npm run build:shared && npm run build --workspace=apps/api",
    "start:worker": "npm run start --workspace=apps/worker",
    "start:api": "npm run start --workspace=apps/api",
    "postinstall": "npm run build:shared",
    "railway:build:worker": "npm run build:worker",
    "railway:build:api": "npm run build:api",
    "railway:start:worker": "npm run start:worker",
    "railway:start:api": "npm run start:api"
  },
  "dependencies": {
    "@nestjs/config": "^4.0.2",
    "@prisma/client": "^7.2.0",
    "prisma": "^7.2.0"
  },
  "devDependencies": {
    "@types/node": "^25.0.3"
  }
}
